include ./partials/head
    include ./partials/header
    form(action="/register" method="post" style="margin:150px;")
      label(for="Email") username:
      input(
        id="email"
        type="text"
        name="email"
        placeholder="email"
        style="width:300px"
        )
      label(for="Password") password:
      input(
        id="password"
        type="text"
        name="password"
        placeholder="password"
        style="width:300px"
        )
      input(type="submit" value="Submit")